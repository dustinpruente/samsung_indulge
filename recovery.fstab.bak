# mount point	fstype		device

/sdcard		vfat		/dev/block/mmcblk0p1
/system		rfs		/dev/block/stl10
/cache		rfs		/dev/block/stl12
/data		rfs		/dev/block/stl11
/boot		mtd		boot
/recovery	mtd		recovery
/bootloader	mtd		bootloader
/radio		mtd		radio
tmpfs           tmpfs           /mnt/sdcard/.android_secure
